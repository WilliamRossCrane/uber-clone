{
  "expo": {
    "name": "uber",
    "slug": "uber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "uberclone",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.uber",
      "splash": {
        "image": "./assets/images/signup-car.png",
        "resizeMode": "contain",
        "backgroundColor": "#2F80ED"
      }
    },
    "android": {
      "package": "com.yourname.uber",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2F80ED"
      },
      "splash": {
        "image": "./assets/images/signup-car.png",
        "resizeMode": "contain",
        "backgroundColor": "#2F80ED"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "EXPO_PUBLIC_GEOAPIFY_API_KEY": "09508cffb70d45658a94c8c9a5027928",
      "EXPO_PUBLIC_GOOGLE_API_KEY": "AIzaSyC7ydcwQicIhilXwe_bax7Mn01J-BK9JGk",
      "EXPO_PUBLIC_SERVER_URL": "https://uber.com"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://uber.com"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
